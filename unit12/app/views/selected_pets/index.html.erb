<h1>Listing selected_pets</h1>

<table>
  <thead>
    <tr>
      <th>Pet</th>
      <th>Cart</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @selected_pets.each do |selected_pet| %>
      <tr>
        <td><%= selected_pet.pet %></td>
        <td><%= selected_pet.cart %></td>
        <td><%= link_to 'Show', selected_pet %></td>
        <td><%= link_to 'Edit', edit_selected_pet_path(selected_pet) %></td>
        <td><%= link_to 'Destroy', selected_pet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Selected pet', new_selected_pet_path %>
